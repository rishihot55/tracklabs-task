<div ng-controller='placesController' ng-init="findPlace('{{place_id}}')">
	<h1 class='text-center'>Find Location</h1>
	<div class='row'>
		<div class='col-md-6 col-md-offset-3'>
			<div class='form-group'>
				<input type='text' class='form-control' id='address' placeholder='Search an existing location' ng-model="address" ng-enter="trackLocation(address)">
			</div>
		</div>
	</div>
	<div class='row'>
		<div class='col-md-8 col-md-offset-2'>
			<div id='map-canvas' style='width: 100%; height: 50vh'></div>
		</div>
	</div>
</div>